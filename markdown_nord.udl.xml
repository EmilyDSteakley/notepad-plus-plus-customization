<?xml version="1.0" encoding="UTF-8" ?>
<NotepadPlus>
    <UserLang name="Markdown [Nord]" ext="md markdown" darkModeTheme="yes" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="yes" allowFoldOfComments="no" foldCompact="no" forcePureLC="2" decimalSeparator="0" />
            <Prefix Keywords1="yes" Keywords2="yes" Keywords3="yes" Keywords4="yes" Keywords5="yes" Keywords6="yes" Keywords7="yes" Keywords8="no" />
        </Settings>

        <KeywordLists>
            <Keywords name="Comments">00# 01 02((EOL)) 03&lt;!-- 04--&gt;</Keywords>
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1">.</Keywords>
            <Keywords name="Numbers, suffix2">.</Keywords>
            <Keywords name="Numbers, range"></Keywords>

            <Keywords name="Operators1">@ &lt; &gt; \&lt; \&gt; \\ \` \* \_ \{ \} \( \) \+ \- \. \! \~ \| |:-: |:--: |:---: | :-: :--: :---: :|: |: :|</Keywords>
            <Keywords name="Operators2">- + :- :-- :--- :</Keywords>

            <Keywords name="Folders in code1, open"></Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close"></Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>

            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>

            <!-- URLs -->
            <Keywords name="Keywords1">../ (../ http:// (http:// https:// (https:// mailto: (mailto: ftp:// (ftp:// ftps:// (ftps:// (/ /</Keywords>

            <!-- Underline headers -->
            <Keywords name="Keywords2">==== ----</Keywords>

            <!-- *** or ___ emphasis -->
            <Keywords name="Keywords3">*** ___</Keywords>

            <!-- Bold -->
            <Keywords name="Keywords4">** __</Keywords>

            <!-- Italic -->
            <Keywords name="Keywords5">* _</Keywords>

            <!-- Bold+italic -->
            <Keywords name="Keywords6">** __</Keywords>
            <Keywords name="Keywords7">* _</Keywords>

            <Keywords name="Keywords8"></Keywords>

            <!-- Delimiters -->
            <Keywords name="Delimiters">00![ 01\ 02] 03``` 03` 03~~~ 04\ 05``` 05((EOL `)) 05~~~ 06*** 07\ 08((EOL ***)) 09** 10\ 11((EOL **)) 12* 13\ 14((EOL *)) 15** 16\ 17((EOL **)) 18* 19\ 20((EOL *)) 21 22 23</Keywords>
        </KeywordLists>

        <Styles>
            <!-- Base text & background -->
            <WordsStyle name="DEFAULT" fgColor="D8DEE9" bgColor="2E3440" fontStyle="0" nesting="0" />

            <!-- ATX Headings (# ... ) → warm coral -->
            <WordsStyle name="COMMENTS" fgColor="E7A07C" bgColor="2E3440" fontStyle="2" nesting="0" />

            <!-- Underline headings (==== / ) → bright mint -->
            <WordsStyle name="LINE COMMENTS" fgColor="7DDFAE" bgColor="2E3440" fontStyle="1" nesting="0" />

            <!-- Ordered list numbers -->
            <WordsStyle name="NUMBERS" fgColor="EBCB8B" bgColor="2E3440" fontStyle="0" nesting="0" />

            <!-- URLs / links → Nord Frost teal -->
            <WordsStyle name="KEYWORDS1" fgColor="88C0D0" bgColor="2E3440" fontStyle="0" nesting="0" />

            <!-- Underline headings' tokens -->
            <WordsStyle name="KEYWORDS2" fgColor="7DDFAE" bgColor="2E3440" fontStyle="1" nesting="0" />

            <!-- Strong emphasis group (*** ___) → frost aqua accent -->
            <WordsStyle name="KEYWORDS3" fgColor="4FCED2" bgColor="2E3440" fontStyle="3" nesting="0" />

            <!-- Bold → strong peach-orange -->
            <WordsStyle name="KEYWORDS4" fgColor="FFB570" bgColor="2E3440" fontStyle="1" nesting="0" />

            <!-- Italic → golden amber -->
            <WordsStyle name="KEYWORDS5" fgColor="FFD060" bgColor="2E3440" fontStyle="2" nesting="0" />

            <!-- Bold+italic → same bold color -->
            <WordsStyle name="KEYWORDS6" fgColor="FFB570" bgColor="2E3440" fontStyle="3" nesting="0" />
            <WordsStyle name="KEYWORDS7" fgColor="FFB570" bgColor="2E3440" fontStyle="3" nesting="0" />

            <!-- Fallback -->
            <WordsStyle name="KEYWORDS8" fgColor="D8DEE9" bgColor="2E3440" fontStyle="0" nesting="0" />

            <!-- Operators / punctuation → frost aqua accent -->
            <WordsStyle name="OPERATORS" fgColor="4FCED2" bgColor="2E3440" fontStyle="1" nesting="0" />

            <WordsStyle name="FOLDER IN CODE1" fgColor="D8DEE9" bgColor="2E3440" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE2" fgColor="D8DEE9" bgColor="2E3440" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="D8DEE9" bgColor="2E3440" fontStyle="0" nesting="0" />

            <!-- Image/link delimiters → teal -->
            <WordsStyle name="DELIMITERS1" fgColor="88C0D0" bgColor="2E3440" fontStyle="2" nesting="0" />

            <!-- Code fences → subdued Nord green -->
            <WordsStyle name="DELIMITERS2" fgColor="A3BE8C" bgColor="2E3440" fontStyle="0" nesting="0" />

            <!-- Extra emphasis group -->
            <WordsStyle name="DELIMITERS3" fgColor="4FCED2" bgColor="2E3440" fontStyle="3" nesting="0" />

            <!-- Bold delimiters -->
            <WordsStyle name="DELIMITERS4" fgColor="FFB570" bgColor="2E3440" fontStyle="1" nesting="65600" />

            <!-- Italic delimiters -->
            <WordsStyle name="DELIMITERS5" fgColor="FFD060" bgColor="2E3440" fontStyle="2" nesting="32800" />

            <!-- Bold+italic delimiters -->
            <WordsStyle name="DELIMITERS6" fgColor="FFB570" bgColor="2E3440" fontStyle="3" nesting="0" />
            <WordsStyle name="DELIMITERS7" fgColor="FFB570" bgColor="2E3440" fontStyle="3" nesting="0" />

            <WordsStyle name="DELIMITERS8" fgColor="D8DEE9" bgColor="2E3440" fontStyle="0" nesting="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
